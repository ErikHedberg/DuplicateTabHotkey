{
	"manifest_version": 2,
	"name": "Duplicate Tab Hotkey",
	"version": "1.0",
	
	"author": "Erik Hedberg",
	
	"description": "Adds a hotkey that lets you easily duplicate the current tab.",
	
	"icons": {
		"128": "icon128.png"
	},
	
	"commands": {
		"duplicate_tab": {
			"suggested_key": {
				"default": "Ctrl+Shift+Space"
			},
			"description": "Duplicate The Current Tab"
		}
	},
	
	"background": {
		"scripts":["Duplicate_Tab.js"]
	},
	
	"permissions": [
		"tabs"
	]
}
